# AdÄ±m 1: KÃ¼tÃ¼phaneler ve CSV yÃ¼kleme

import pandas as pd
import numpy as np
import tensorflow as tf
from itertools import combinations

# EÄŸer Colab kullanÄ±yorsan, dosya yÃ¼kleme iÃ§in:
from google.colab import files
import io

print("ğŸ“‚ LÃ¼tfen SÃ¼per Loto CSV dosyanÄ±zÄ± yÃ¼kleyin (Date, Num1~Num6 sÃ¼tunlarÄ± olmalÄ±).")

uploaded = files.upload()
filename = next(iter(uploaded))

# CSV'yi yÃ¼kle ve tarihi datetime yap, sayÄ±larÄ± listeye al
df = pd.read_csv(io.StringIO(uploaded[filename].decode('utf-8')))
df['Date'] = pd.to_datetime(df['Date'])
df['Numbers'] = df[['Num1','Num2','Num3','Num4','Num5','Num6']].values.tolist()

print(f"âœ… Veri yÃ¼klendi. Toplam Ã§ekiliÅŸ sayÄ±sÄ±: {len(df)}")
print(df.head())
